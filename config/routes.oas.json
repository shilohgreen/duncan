{
  "openapi": "3.1.0",
  "info": {
    "version": "1.0.0",
    "title": "My Zuplo API"
  },
  "paths": {
    "/hello": {
      "get": {
        "summary": "Hello World",
        "description": "A simple hello world endpoint",
        "operationId": "hello-world",
        "x-zuplo-route": {
          "corsPolicy": "none",
          "handler": {
            "export": "default",
            "module": "$import(./modules/hello-world)"
          },
          "policies": {
            "inbound": []
          }
        }
      }
    }
  }
}
